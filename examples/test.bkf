alias CS- = {"ctrl", "shift"}

layer fancy:
| "s" "d" "f" *r => {"leftctrl", "leftshift", r}
| "j" "k" "l" *r => {"rightctrl", "rightshift", r}
| "s" "d" *r => {"leftshift", r}
| "k" "l" *r => {"rightshift", r}
| "s" "f" *r => {"leftctrl", r}
| "j" "l" *r => {"rightctrl", r}
| "q" "e" => "esc"
| "g" "h" => "backspace"

